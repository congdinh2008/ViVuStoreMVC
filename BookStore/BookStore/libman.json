{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "defaultDestination": "wwwroot/lib",
  "libraries": [
    {
      "library": "twitter-bootstrap@4.1.1",
      "files": [ "css/bootstrap.min.css", "css/bootstrap.min.css.map", "js/bootstrap.min.js", "js/bootstrap.min.js.map" ],
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "library": "jquery@3.3.1",
      "files": [ "jquery.min.js", "jquery.min.map" ],
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "library": "popper.js@1.14.3",
      "files": [ "umd/popper.min.js","umd/popper.min.js.map" ],
      "destination": "wwwroot/lib/popper.js/"
    }
  ]
}